{
  "expo": {
    "name": "expo-project",
    "slug": "expo-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "expo-project",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.muhammadshahms.expoproject"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.muhammadshahms.expoproject"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fa7ec9a3-a1f8-4c6c-b82a-2aefaef96920"
      }
    },
    "owner": "muhammadshahms",
    "updates": {
      "url": "https://u.expo.dev/fa7ec9a3-a1f8-4c6c-b82a-2aefaef96920"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}